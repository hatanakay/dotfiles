[user]
	name = hatanakay
	email = hatanaka.yoichi@gmail.com
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
    grep = auto
[alias]
    ch = checkout
    cm = commit -m
    ca = commit -a
    st = status
    br = branch
    df = diff
    wh = whatchanged
    hi = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    gr = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
    stt = diff --stat
    su = log --stat --summary
    co = checkout
    ci = commit -v
    di = diff
    puhs = push
    psuh = push
    pus = push
    puh = push
    pl = !git pull && git submodule update --init
[core]
    quotepath = false
    editor = vim
	excludesfile = /Users/h222/.gitignore_global
[http]
    sslVerify = false
[tig "bind"]
    generic = g move-first-line
    generic = G move-last-line
[merge]
    tool = vimdiff
[push]
    default = current
[github]
	user = hatanakay
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.localized/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
