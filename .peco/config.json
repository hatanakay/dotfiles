{
    "keymap": {
        "C-j": "peco.SelectNext",
        "C-k": "peco.ToggleSelectionAndSelectNext"
    },
    "Style": {
        "Basic": ["on_default", "default"],
        "Selected": ["underline", "on_red", "white"],
        "SavedSelection": ["on_yellow", "black"],
        "Query": ["white", "bold"], 
        "Matched": ["red", "on_black", "bold"]
    },
    "Prompt": ">", 
    "CustomMatcher": {
        "Migemo": ["migemogrep", "$QUERY"]
    }
}
